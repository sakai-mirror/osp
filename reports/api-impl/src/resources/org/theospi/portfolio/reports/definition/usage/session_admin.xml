<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
	Notes:

	Each report MUST have a different idString.  This is how saved reports are
	linked back to the report definition

    ALL YOUR BASE ARE BELONG TO US!
-->
<beans>


    <bean id="sessionAdminReport"
        class="org.theospi.portfolio.reports.model.ReportDefinition">

        <property name="idString">
            <value>sessionAdmin-000-111-222</value>
        </property>
        <property name="usesWarehouse"><value>true</value></property>
        <property name="title">
            <value>
                Session Report
            </value>
        </property>
       <property name="query">
            <list>
               <value>
                  select count(*) 'totalVisits' from dw_session where startTime between ? and ?
               </value>
                 <value>
                  select count(distinct userId) 'uniqueVisits' from dw_session  where startTime between ? and ?
               </value>
               <value>
                   select avg(endTime - startTime) 'avgSessionInSeconds'  from dw_session  where startTime between ? and ?
               </value>
               <value>
                   select Distinct(user_eid), first_name, last_name from dw_users where user_id not  in (select distinct userId from dw_session where startTime between ? and ?)
               </value>
            </list>
        </property>
        <property name="description">
            <value>
                Session report
            </value>
        </property>
        <property name="siteType"><!-- some acceptable values: admin,course,project,cig  -->
            <value>admin, portfolioAdmin</value>
        </property>
        <property name="role">
            <value>admin, Program Admin</value>
        </property>
        <property name="reportDefinitionParams">
            <list>
                <bean class="org.theospi.portfolio.reports.model.ReportDefinitionParam">
                    <property name="title"><value>Start Date:</value></property>
                    <property name="paramName"><value>startdate</value></property>
                    <property name="description"><value></value></property>
                    <property name="type"><value>date</value></property>
                    <property name="valueType"><value>fillin</value></property>
                    <property name="value"><value></value></property>
                </bean>
                <bean class="org.theospi.portfolio.reports.model.ReportDefinitionParam">
                    <property name="title"><value>End Date:</value></property>
                    <property name="paramName"><value>enddate</value></property>
                    <property name="description"><value></value></property>
                    <property name="type"><value>date</value></property>
                    <property name="valueType"><value>fillin</value></property>
                    <property name="value"><value></value></property>
                </bean>
            </list>
        </property>

        <property name="xsls">
            <list>
                <bean id="internalXML" class="org.theospi.portfolio.reports.model.ReportXsl">
                    <property name="xslLink"><value>/org/theospi/portfolio/reports/xsl/shared/internalXML.xsl</value></property>
                    <property name="title"><value>xml</value></property>
                    <property name="isExport"><value>false</value></property>
                </bean>
                <bean id="reportXsl1.1" class="org.theospi.portfolio.reports.model.ReportXsl">
                    <property name="xslLink"><value>/org/theospi/portfolio/reports/xsl/shared/default.xsl</value></property>
                    <property name="title"><value>Report Result View 1</value></property>
                    <property name="isExport"><value>false</value></property>
                </bean>
            </list>
           
        </property>

        <property name="defaultXsl">
                <bean class="org.theospi.portfolio.reports.model.ReportXsl">
                    <property name="xslLink"><value>/org/theospi/portfolio/reports/xsl/shared/internalXML.xsl</value></property>
                    <property name="isExport"><value>false</value></property>
                </bean>
        </property>

    </bean>
    </beans>