                    <entry key="formName">
                        <value>Address</value>
                    </entry>
                    <entry key="formFields">
                        <list>
                            <value>state</value>
                            <value>city</value>
                        </list>
                    </entry>
                </map>
            </list>
        </property>
        -->
        <property name="xsls">
            <list>
                <value>/reportxsl/report1.1.xsl</value>
                <value>/reportxsl/report1.2.xsl</value>
                <value>/reportxsl/report1.3.xsl</value>
            </list>
        </property>

    </bean>

</beans>
