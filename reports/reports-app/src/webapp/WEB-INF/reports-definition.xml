        <property name="description">
            <value>
                This report is to view the portfolio performance
            </value>
        </property>
        <property name="defaultXsl">
            <value>/reportxsl/report1.1.xsl</value>
        </property>

        <property name="params">
            <list>
                <map>
                    <entry key="paramName"><value>[user]</value></entry>
                    <entry key="description"><value>
                            specify a user
                        </value></entry>
                    <entry key="type"><!-- int/float/date/string --><value>string</value></entry>
                    <entry key="valueType"><!-- fillin/set/multiset/sql/multisql/static -->
                        <value>static</value>
                    </entry>
                    <entry key="value"><!-- (validation rules)/[elem1, elem2, elem3,
                            etc]/ select username from worksite/value -->
                        <value>
                        admin
                        </value>
                    </entry>
                </map>
            </list>
        </property>
