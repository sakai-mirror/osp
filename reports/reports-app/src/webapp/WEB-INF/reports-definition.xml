<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>


    <bean id="report1"
        class="org.theospi.api.app.reports.ReportDefinition">

        <property name="reportDefId">
            <value>8b9df0-d90f-d898f-f89f8d78d7f8</value>
        </property>
        <property name="title">
            <value>
                Report Variable Type Test
            </value>
        </property>
        <property name="query">
            <value>
                select * from sakai_site
            </value>
        </property>
        <property name="keywords">
            <value>CIG user portfolio etc</value>
        </property>
        <property name="description">
            <value>
                Tests each of the parameter variable types
            </value>
        </property>
        <property name="defaultXsl">
            <value>/reportxsl/report1.1.xsl</value>
        </property>

        <property name="params">
            <list>
                <map>
                    <entry key="paramName"><value>[fillin]</value></entry>
                    <entry key="description"><value>
                            selects the user whom owns the portfolio
                        </value></entry>
                    <entry key="type"><!-- int/float/date/string --><value>string</value></entry>
                    <entry key="valueType"><!-- fillin/set/multiset/sql/multisql/static -->
                        <value>fillin</value>
                    </entry>
                    <entry key="value"><!-- (validation rules)/[elem1, elem2, elem3,
                            etc]/ select username from worksite/value -->
                        <value></value>
                    </entry>
                </map>
                <map>
                    <entry key="paramName"><value>[set]</value></entry>
                    <entry key="description"><value>
                            selects the user whom owns the portfolio
                        </value></entry>
                    <entry key="type"><!-- int/float/date/string --><value>string</value></entry>
                    <entry key="valueType"><!-- fillin/set/multiset/sql/multisql/static -->
                        <value>set</value>
                    </entry>
                    <entry key="value"><!-- (validation rules)/[elem1, elem2, elem3,
                            etc]/ select username from worksite/value -->
                        <value>
                        [dog, cat, car, apple, turkey, paper, ]
                        </value>
                    </entry>
                </map>
                <map>
                    <entry key="paramName"><value>[multiset]</value></entry>
                    <entry key="description"><value>
                            selects the user whom owns the portfolio
                        </value></entry>
                    <entry key="type"><!-- int/float/date/string --><value>string</value></entry>
                    <entry key="valueType"><!-- fillin/set/multiset/sql/multisql/static -->
                        <value>multiset</value>
                    </entry>
                    <entry key="value"><!-- (validation rules)/[elem1, elem2, elem3,
                            etc]/ select username from worksite/value -->
                        <value>
                        [(dog; A Dog), (cat), (car; BMW), (apple; Red Apple), turkey, paper, {userid}]
                        </value>
                    </entry>
                </map>
                <map>
                    <entry key="paramName"><value>[sql]</value></entry>
                    <entry key="description"><value>
                            allows a user to select from the worksites they have access
                        </value></entry>
                    <entry key="type"><!-- int/float/date/string --><value>string</value></entry>
                    <entry key="valueType"><!-- fillin/set/multiset/sql/multisql/static -->
                        <value>sql</value>
                    </entry>
                    <entry key="value"><!-- (validation rules)/[elem1, elem2, elem3,
                            etc]/ select username from worksite/value -->
                        <value>
                        select site_id from sakai_site_user where user_id="{userid}"
                        </value>
                    </entry>
                </map>
                <map>
                    <entry key="paramName"><value>[multisql]</value></entry>
                    <entry key="description"><value>
                            allows a user to select from the worksites they have access
                        </value></entry>
                    <entry key="type"><!-- int/float/date/string --><value>string</value></entry>
                    <entry key="valueType"><!-- fillin/set/multiset/sql/multisql/static -->
                        <value>multisql</value>
                    </entry>
                    <entry key="value"><!-- (validation rules)/[elem1, elem2, elem3,
                            etc]/ select username from worksite/value -->
                        <value>
                        select site_id from sakai_site_user where user_id="{userid}"
                        </value>
                    </entry>
                </map>
                <map>
                    <entry key="paramName"><value>[static]</value></entry>
                    <entry key="description"><value>
                            a static parameter
                        </value></entry>
                    <entry key="type"><!-- int/float/date/string --><value>string</value></entry>
                    <entry key="valueType"><!-- fillin/set/multiset/sql/multisql/static -->
                        <value>static</value>
                    </entry>
                    <entry key="value"><!-- (validation rules)/[elem1, elem2, elem3,
                            etc]/ select username from worksite/value -->
                        <value>
                        {userid} - {username}
                        </value>
                    </entry>
                </map>
            </list>
        </property>
        <!-- 
        <property name="forms">
            <list>
                <map>
                    <entry key="formName">
                        <value>Address</value>
                    </entry>
                    <entry key="formFields">
                        <list>
                            <value>state</value>
                            <value>city</value>
                        </list>
                    </entry>
                </map>
            </list>
        </property>
        -->
        <property name="xsls">
            <list>
                <value>/reportxsl/report1.1.xsl</value>
                <value>/reportxsl/report1.2.xsl</value>
                <value>/reportxsl/report1.3.xsl</value>
            </list>
        </property>

    </bean>
    
    
    
    <bean id="report2"
        class="org.theospi.api.app.reports.ReportDefinition">

        <property name="reportDefId">
            <value>8b9df0-d90f-d898f-f89f8d78d7f9</value>
