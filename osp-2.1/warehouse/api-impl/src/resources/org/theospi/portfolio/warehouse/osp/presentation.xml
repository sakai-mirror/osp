<tables>

  <table>
    <name>DW_PRESENTATION</name>
    <desc>Presentation Datawarehouse table</desc>
    <columns>
       <column>
         <name>id</name>
         <type>VARCHAR</type>
         <param>36</param>
       </column>
       <column>
         <name>owner</name>
         <type>VARCHAR</type>
         <param>255</param>
       </column>
       <column>
         <name>name</name>
         <type>VARCHAR</type>
         <param>255</param>
       </column>
    </columns>
    <not-null>id</not-null>
    <primary-key>id</primary-key>
  </table>

   <table>
     <name>DW_PRES_ITEM</name>
     <desc>Presentation Item Datawarehouse table</desc>
     <columns>
        <column>
          <name>presentation_id</name>
          <type>VARCHAR</type>
          <param>36</param>
        </column>
        <column>
          <name>artifact</name>
          <type>VARCHAR</type>
          <param>36</param>
        </column>
     </columns>
   </table>

