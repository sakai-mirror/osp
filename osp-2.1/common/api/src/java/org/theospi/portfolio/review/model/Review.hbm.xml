<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="org.theospi.portfolio.review.model">

   <class name="Review" table="osp_review" lazy="false" batch-size="10">
      <cache usage="nonstrict-read-write"/>
      <id name="id" length="36" type="org.sakaiproject.metaobj.shared.IdType" column="id">
         <generator class="org.sakaiproject.metaobj.shared.IdGenerator"/>
      </id>
      <property name="reviewContent" length="36" type="org.sakaiproject.metaobj.shared.IdType" column="review_content_id" />
      <property name="title" type="string" column="title" not-null="false"/>
      <property name="creator" column="creator_id" not-null="true"
         type="org.sakaiproject.metaobj.shared.mgt.AgentUserType"/>
      <property name="visibility" type="int" not-null="true"/>
      <property name="created" type="java.util.Date" update="false" not-null="true"/>
      <property name="siteId" type="string" update="false" length="36" column="site_id" not-null="true"/>
      <property name="parent" length="36" type="string" column="parent_id" />
      <property name="deviceId" length="36" type="string" column="review_device_id" />
      <property name="type" type="int" column="review_type" not-null="true"/>
      <property name="securityQualifier" type="org.sakaiproject.metaobj.shared.IdType" />
      <property name="securityViewFunction" type="string" not-null="true"/>
      <property name="securityEditFunction" type="string" not-null="true"/>
   </class>

   
</hibernate-mapping>
