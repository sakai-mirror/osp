<table class="listHier" cellspacing="0" >
   <thead>
      <tr>
         <th scope="col">Title</th>
         <th scope="col">Created By</th>
         <th scope="col">Creation Date</th>
      </tr>
   </thead>
   <tbody>
      <c:forEach var="object" items="${objectList}" varStatus="loopStatus">
         <tr>
            <td>
               <c:choose>
                  <c:when test="${object.visibility==1 and object.creator.id==currentUser}">
                     Private <c:out value="${object.title}" />
                  </c:when>
                  <c:when test="${object.visibility==2 and (object.creator.id==currentUser or currentUser==cell.matrix.owner.id)}">
                     Shared <c:out value="${object.title}" />
                  </c:when>
                  <c:when test="${object.visibility==3}">
                     Public <c:out value="${object.title}" />
                  </c:when>
               </c:choose>
            </td>
            <td>
               <c:out value="${object.creator.displayName}" />
            </td>
            <td>
               <fmt:formatDate value="${object.created}" pattern="MM-dd-yyyy" />
            </td>
         </tr>
      </c:forEach>
   </tbody>
</table>