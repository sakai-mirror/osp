<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans default-lazy-init="false">

   <bean id="org.theospi.portfolio.warehouse.intf.ChildWarehouseTask.help.glossary"
   class="org.theospi.portfolio.warehouse.impl.BaseChildWarehouseTask">
    <property name="fields">
       <list>
          <ref bean="org.theospi.portfolio.warehouse.intf.PropertyAccess.id" />
          <bean class="org.theospi.portfolio.warehouse.impl.BeanPropertyAccess">
        <property name="propertyName"><value>worksiteId</value></property>
          </bean>
          <bean class="org.theospi.portfolio.warehouse.impl.BeanPropertyAccess">
        <property name="propertyName"><value>term</value></property>
          </bean>
          <bean class="org.theospi.portfolio.warehouse.impl.BeanPropertyAccess">
         <property name="propertyName"><value>description</value></property>
          </bean>
        </list>
    </property>
    <property name="insertStmt">
       <value>insert into dw_help_glossary  (id, worksite_id, term, description)
                   values (?, ?, ?, ?)
      </value>
    </property>
    <property name="clearStmt">
       <value>delete from dw_help_glossary</value>
    </property>
   </bean>

</beans>